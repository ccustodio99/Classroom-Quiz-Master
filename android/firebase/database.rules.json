{
  "rules": {
    "presence": {
      "$classId": {
        "$userId": {
          ".read": "auth != null && auth.token.org == $classId.split('-')[0]",
          ".write": "auth != null && auth.uid == $userId"
        }
      }
    },
    "live": {
      "$sessionId": {
        ".read": "auth != null",
        ".write": "auth != null"
      }
    }
  }
}
